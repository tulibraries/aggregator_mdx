        <x:expect label="map from contributor if NOT with 'provider' text" test="oai_dc:dc/dcterms:contributor = 'Board of Fish Commissioners (Creator)'"/>
      </x:scenario>
      <x:scenario label="contributor processing testing delimiters">
        <x:context>
          <oai:record>
            <metadata>
              <oai_dc:dc xsi:schemaLocation="http://www.openarchives.org/OAI/2.0/oai_dc/ http://www.openarchives.org/OAI/2.0/oai_dc.xsd"
                xmlns="http://purl.org/dc/elements/1.1/"
