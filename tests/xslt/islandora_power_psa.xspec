      <x:context href="../../fixtures/islandora_power_psa.xml"/>
      <x:expect label="identifier generated intProvider name" test="oai_dc:dc/dpla:intermediateProvider = 'POWER Library as sponsor and HSLC as maintainer'"/>
    </x:scenario>

    <!-- isShownAt (map) -->
    <x:scenario label="isShownAt processing">
      <x:context href="../../fixtures/islandora_power_psa.xml"/>
      <x:expect label="last identifier maps to edm:isShownAt" test="oai_dc:dc/edm:isShownAt = 'https://digitalarchives.powerlibrary.org/psa/islandora/object/psa%3Abfcm_2646'"/>
    </x:scenario>

    <!-- Preview (map) -->
    <x:scenario label="preview processing">
      <x:context href="../../fixtures/islandora_power_psa.xml"/>
      <x:expect label="preview generates and maps" test="oai_dc:dc/edm:preview = 'https://digitalarchives.powerlibrary.org/psa/islandora/object/psa%3Abfcm_2646/datastream/TN/view/Board%20of%20Fish%20Commissioners%2C%20Minutes%20%28Roll%206625%2C%20Part%201%29.jpg'"/>
    </x:scenario>

    <!-- isPartOf-->
    <x:scenario label="SetSpec is looked up and matched to collection name">
      <x:context href="../../fixtures/islandora_power_psa.xml"/>
        <x:expect label="SetSpec matches collection in lookup.xsl"
          test="oai_dc:dc/dcterms:isPartOf = 'Board of Fish Commission (PA) Minutes, 1924-1969'"/>
    </x:scenario>

    <!-- Hub -->
    <x:scenario label="hard coded hub name">
      <x:context href="../../fixtures/islandora_power_psa.xml"/>
        <x:expect label="hard coded hub name" test="oai_dc:dc/edm:provider = 'PA Digital'"/>
    </x:scenario>

<!-- Generic crosswalk checks -->
  
    <!-- ampersand normalization -->
    <x:scenario label="fields with broken ampersand encoding normalize to correct string">
      <x:context href="../../fixtures/islandora_historicpitt.xml"/>
      <x:expect label="title value normalizes"
        test="oai_dc:dc/dcterms:title = 'Boggs &amp; Buhl Department Store (Pittsburgh, Pa.)'"/>
      <x:expect label="creator value normalizes"
        test="oai_dc:dc/dcterms:creator = 'Pittsburgh City Photographer &amp; Co.'"/>
      <x:expect label="subject value normalizes"
        test="oai_dc:dc/dcterms:subject = 'Overbrook Elementary School &amp; Co. (Pittsburgh, Pa.)'"/>
      <x:expect label="description value normalizes"
        test="oai_dc:dc/dcterms:description = 'A view of the Overbrook Elementary School &amp; Co. The school closed in 2002.'"/>
      <x:expect label="relation value normalizes"
        test="oai_dc:dc/dcterms:relation = 'Pittsburgh City Photographer &amp; Co. Collection, 1901-2002'"/>
    </x:scenario>
  
    <!-- Title -->
    <x:scenario label="dc:title processing">
        <x:context>
            <oai:record>
                <metadata>
                    <oai_dc:dc xsi:schemaLocation="http://www.openarchives.org/OAI/2.0/oai_dc/ http://www.openarchives.org/OAI/2.0/oai_dc.xsd"
