<!-- Institution specific checks -->

    <!-- Identifier (map) -->
    <x:scenario label="identifier">
      <x:context href="../../fixtures/islandora_power_psa.xml"/>
      <x:expect label="dummy identifier generates and incoming identifier does not map" test="oai_dc:dc/dcterms:identifier = 'padig:POWER-psa_3Abfcm_2646'"/>
    </x:scenario>

    <!-- Data Provider (generate and map) -->
    <x:scenario label="data provider processing">
      <x:scenario label="dc:contributor contains (provider)">
      <x:context href="../../fixtures/islandora_power_psa.xml"/>
        <x:expect label="map from contributor with 'provider' text" test="oai_dc:dc/edm:dataProvider = 'Pennsylvania State Archives'"/>
    </x:scenario>
      <x:scenario label="dc:contributor contains (provider) and an &amp;">
          <x:context>
            <oai:record>
              <metadata>
                <oai_dc:dc xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:dc="http://purl.org/dc/elements/1.1/"
                  xmlns:dcterms="http://purl.org/dc/terms/"
                  xmlns:dpla="http://dp.la/about/map/"
                  xmlns:edm="http://www.europeana.eu/schemas/edm/"
                  xmlns:oai="http://www.openarchives.org/OAI/2.0/"
                  xmlns:oai_dc="http://www.openarchives.org/OAI/2.0/oai_dc/"
                  xmlns:oai_qdc="http://worldcat.org/xmlschemas/qdc-1.0/"
                  xmlns:oclc="http://purl.org/oclc/terms/"
                  xmlns:oclcdc="http://worldcat.org/xmlschemas/oclcdc-1.0/"
                  xmlns:oclcterms="http://purl.org/oclc/terms/"
